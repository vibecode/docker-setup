server {
   listen 80;
   
   server_name docker-setup.com;

   location / {
       proxy_pass http://frontend:3000;
   }

   location /api {
       proxy_pass http://api:3001;
       rewrite ^/api/(.*) /$1 break;
   }

   location /auth/api {
       proxy_pass http://auth:3002;
       rewrite ^/auth/api/(.*) /$1 break;
   }
}
